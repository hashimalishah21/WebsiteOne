<h2>Members (<%= @members.count %>)</h2>
<% if members.empty? %>
  <p style="margin-top: 20px;">No members in project <strong><%= @project.title %></strong></p>
<% else %>
  <ul class="media-list row">
  <% members.each do |member| %>
    <% present member do |presenter| %>
      <li class="media-item">
        <a class="user-preview" href="<%= user_path(member) %>" data-toggle="tooltip"
          data-placement="top" title="<%= presenter.display_name %>">
          <img src="<%= presenter.gravatar_src %>" class="projects-user-avatar">
          <div class="media-body index-summary">
            <p class="media-heading"><%= presenter.display_name %></p>
            <% if presenter.has_title? %>
              <p><%= presenter.title_list %></p>
            <% end %>
            <% if member.country? %>
              <p><%= presenter.country %></p>
            <% end %>
          </div>
        </a>
      </li>
    <% end %>
  <% end %>
  </ul>
<% end %>




