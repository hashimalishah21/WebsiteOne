<%= content_for :content do %>
  <div id="with-sidebar-container"class="container">
    <nav id="sidebarnav" class="col-sm-3" role="complementary">
    <%= render partial: 'layouts/sidebar', locals: { project: @project, document: @document } %>
    </nav>
    <div id="not-sidebar" class="col-sm-9">
      <div id="not-sidebar-content">
       <%= yield %>
     </div>
    </div>
  </div>
<% end %>
<%= render template: 'layouts/application' %>
